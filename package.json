        "globals": "^16.4.0",
        "i18next": "^25.5.3",
        "joi": "^18.0.1",
        "only-allow": "^1.2.1",
        "prettier": "^3.6.2",
        "prettier-plugin-svelte": "^3.4.0",
        "publint": "^0.3.13",
        "sass": "^1.93.2",
        "superstruct": "^2.0.2",
        "svelte": "^5.39.8",
        "svelte-check": "^4.3.2",
        "svelte-french-toast": "^1.2.0",
        "sveltekit-flash-message": "^2.4.6",
        "sveltekit-rate-limiter": "^0.7.0",
        "throttle-debounce": "^5.0.2",
        "tslib": "^2.8.1",
